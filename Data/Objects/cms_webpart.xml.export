="500" columntype="text" guid="3362eb82-ea0f-4924-9e80-beea49ab7d88" publicfield="false" visible="true"&gt;&lt;properties&gt;&lt;fieldcaption&gt;Alternating transformation&lt;/fieldcaption&gt;&lt;fielddescription&gt;Sets the transformation used to display even wizard steps in the navigation.&lt;/fielddescription&gt;&lt;/properties&gt;&lt;settings&gt;&lt;controlname&gt;selecttransformation&lt;/controlname&gt;&lt;/settings&gt;&lt;/field&gt;&lt;field allowempty="true" column="FirstTransformationName" columnsize="500" columntype="text" guid="20af6fe1-a4ca-4a1d-b24f-1305201fa96c" publicfield="false" visible="true"&gt;&lt;properties&gt;&lt;fieldcaption&gt;First item transformation&lt;/fieldcaption&gt;&lt;fielddescription&gt;Sets the transformation used to display the first wizard step in the navigation.&lt;/fielddescription&gt;&lt;/properties&gt;&lt;settings&gt;&lt;controlname&gt;selecttransformation&lt;/controlname&gt;&lt;/settings&gt;&lt;/field&gt;&lt;field allowempty="true" column="LastTransformationName" columnsize="500" columntype="text" guid="0dc2f22d-b645-4d47-97b5-64200069f692" publicfield="false" visible="true"&gt;&lt;properties&gt;&lt;fieldcaption&gt;Last item transformation name&lt;/fieldcaption&gt;&lt;fielddescription&gt;Sets the transformation used to display the last wizard step in the navigation.&lt;/fielddescription&gt;&lt;/properties&gt;&lt;settings&gt;&lt;controlname&gt;selecttransformation&lt;/controlname&gt;&lt;/settings&gt;&lt;/field&gt;&lt;field allowempty="true" column="LastInactiveTransformationName" columnsize="500" columntype="text" guid="8f0f9591-41c8-4c4d-9228-a82f54ecacef" publicfield="false" resolvedefaultvalue="False" visible="true"&gt;&lt;properties&gt;&lt;fieldcaption&gt;Last inactive item transformation&lt;/fieldcaption&gt;&lt;fielddescription&gt;Sets the transformation used to display the last wizard step if it is inactive, i.e. hasn't been reached by the user yet. The wizard only marks steps as inactive if the 'Restrict step order' property of the Page wizard manager web part is enabled.&lt;/fielddescription&gt;&lt;/properties&gt;&lt;settings&gt;&lt;controlname&gt;selecttransformation&lt;/controlname&gt;&lt;/settings&gt;&lt;/field&gt;&lt;/form&gt;</WebPartProperties>
