      <WebPartName>Wizard</WebPartName>
      <WebPartDisplayName>Wizard layout</WebPartDisplayName>
      <WebPartDescription>Provides a layout divided into multiple steps. The wizard displays one step at a time and allows users to navigate through them in consecutive order. The content of each step is defined through a separate web part zone.</WebPartDescription>
      <WebPartFileName>Layouts/Wizard/Wizard.ascx</WebPartFileName>
      <WebPartProperties>&lt;form version="2"&gt;&lt;category name="Layout settings"&gt;&lt;properties&gt;&lt;visible&gt;True&lt;/visible&gt;&lt;/properties&gt;&lt;/category&gt;&lt;field allowempty="true" column="AllowDesignMode" columntype="boolean" guid="217dfab8-3e01-46be-a0b0-cfe4d67eae16" publicfield="false" spellcheck="false" visibility="none" visible="true"&gt;&lt;properties&gt;&lt;defaultvalue&gt;true&lt;/defaultvalue&gt;&lt;fieldcaption&gt;Allow design mode&lt;/fieldcaption&gt;&lt;fielddescription&gt;If checked, users will be able to add, remove and switch between the steps of the wizard directly on the Design tab of the Pages application. This also makes it possible to resize the dimensions of the wizard by dragging its borders. If disabled, the wizard layout will need to be managed through the property configuration dialog of the web part.&lt;/fielddescription&gt;&lt;/properties&gt;&lt;settings&gt;&lt;controlname&gt;checkboxcontrol&lt;/controlname&gt;&lt;/settings&gt;&lt;/field&gt;&lt;field allowempty="true" column="Width" columnsize="100" columntype="text" guid="f216852f-301e-4d6c-9649-b263ded379b1" publicfield="false" spellcheck="false" visibility="none" visible="true"&gt;&lt;properties&gt;&lt;fieldcaption&gt;Wizard width&lt;/fieldcaption&gt;&lt;fielddescription&gt;Sets the width of the entire wizard, specified as a CSS style value, e.g. 200px or 50%. Leave the value empty for full width.&lt;/fielddescription&gt;&lt;/properties&gt;&lt;settings&gt;&lt;AutoCompleteEnableCaching&gt;False&lt;/AutoCompleteEnableCaching&gt;&lt;AutoCompleteFirstRowSelected&gt;False&lt;/AutoCompleteFirstRowSelected&gt;&lt;AutoCompleteShowOnlyCurrentWordInCompletionListItem&gt;False&lt;/AutoCompleteShowOnlyCurrentWordInCompletionListItem&gt;&lt;controlname&gt;textboxcontrol&lt;/controlname&gt;&lt;FilterMode&gt;False&lt;/FilterMode&gt;&lt;Trim&gt;False&lt;/Trim&gt;&lt;/settings&gt;&lt;/field&gt;&lt;category name="Steps"&gt;&lt;properties&gt;&lt;visible&gt;True&lt;/visible&gt;&lt;/properties&gt;&lt;/category&gt;&lt;field column="Steps" columntype="integer" guid="01195731-8896-46f4-9ad1-631fe93682eb" publicfield="false" visibility="none" visible="true"&gt;&lt;properties&gt;&lt;defaultvalue&gt;3&lt;/defaultvalue&gt;&lt;fieldcaption&gt;Number of steps&lt;/fieldcaption&gt;&lt;fielddescription&gt;Specifies how many steps will be included in the wizard. The content of each step can be entered into a separate web part zone.&lt;/fielddescription&gt;&lt;/properties&gt;&lt;settings&gt;&lt;AutoCompleteEnableCaching&gt;False&lt;/AutoCompleteEnableCaching&gt;&lt;AutoCompleteFirstRowSelected&gt;False&lt;/AutoCompleteFirstRowSelected&gt;&lt;AutoCompleteShowOnlyCurrentWordInCompletionListItem&gt;False&lt;/AutoCompleteShowOnlyCurrentWordInCompletionListItem&gt;&lt;controlname&gt;textboxcontrol&lt;/controlname&gt;&lt;FilterMode&gt;False&lt;/FilterMode&gt;&lt;Trim&gt;False&lt;/Trim&gt;&lt;/settings&gt;&lt;/field&gt;&lt;field allowempty="true" column="ActiveStep" columntype="integer" guid="a93a985b-f5e4-456c-a7e7-d2c92356f8aa" publicfield="false" visibility="none" visible="true"&gt;&lt;properties&gt;&lt;fieldcaption&gt;Active step number&lt;/fieldcaption&gt;&lt;fielddescription&gt;Determines which step will initially be shown by the wizard. Enter the number of the step that you wish to use. If left empty, the first step is loaded by default.&lt;/fielddescription&gt;&lt;/properties&gt;&lt;settings&gt;&lt;AutoCompleteEnableCaching&gt;False&lt;/AutoCompleteEnableCaching&gt;&lt;AutoCompleteFirstRowSelected&gt;False&lt;/AutoCompleteFirstRowSelected&gt;&lt;AutoCompleteShowOnlyCurrentWordInCompletionListItem&gt;False&lt;/AutoCompleteShowOnlyCurrentWordInCompletionListItem&gt;&lt;controlname&gt;textboxcontrol&lt;/controlname&gt;&lt;FilterMode&gt;False&lt;/FilterMode&gt;&lt;Trim&gt;False&lt;/Trim&gt;&lt;/settings&gt;&lt;/field&gt;&lt;category name="Row settings"&gt;&lt;properties&gt;&lt;caption&gt;Header&lt;/caption&gt;&lt;visible&gt;True&lt;/visible&gt;&lt;/properties&